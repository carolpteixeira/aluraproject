let contador1 = 1;

while (contador1 <= 10) {
    alert(`Número ${contador1}`);
    contador1 += 1;
}

let contador2 = 10;

while (contador2 >=0) {
    alert(`Número ${contador2}`);
    contador2 -= 1;
}

let contador3 = prompt('Digite um número:');

while (contador3 >= 0) {
    alert(`Número ${contador3}`);
    contador3 -= 1;
}

let numero1 = prompt('Digite um número:');
let contador4 = 0;

while (numero1 >= contador4) {
    alert(`Número ${contador4}`);
    contador4 += 1;
}
